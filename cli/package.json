{
    "name": "@farhod_dev/super-ui-cli",
    "version": "0.1.1",
    "description": "CLI tool for Super UI component library",
    "author": {
        "name": "Farhod Soyilov",
        "url": "https://github.com/Farhodoff"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Farhodoff/components-main.git",
        "directory": "cli"
    },
    "license": "MIT",
    "bin": {
        "super-ui": "./dist/index.js"
    },
    "files": [
        "dist",
        "templates"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs --dts --clean",
        "dev": "tsup src/index.ts --format cjs --watch",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "commander": "^11.1.0",
        "chalk": "^5.3.0",
        "ora": "^8.0.1",
        "fs-extra": "^11.2.0",
        "execa": "^8.0.1",
        "prompts": "^2.4.2"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.10.6",
        "@types/prompts": "^2.4.9",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "cli",
        "components",
        "react",
        "ui",
        "tailwind",
        "radix-ui"
    ]
}